public class Main {
    public static void main(String[] args) {
        myArrayList myArrayList = new myArrayList(50, 0.2);

        // Add some fractions to the ArrayList
        myArrayList.add(new Fraction(1, 2));
        myArrayList.add(new Fraction(3, 4));
        myArrayList.add(new Fraction(5, 6));

        // Print the ArrayList
        System.out.println(myArrayList);

        // Find the index of a fraction
        int index = myArrayList.indexOf(new Fraction(3, 4));
        System.out.println("Index of fraction 3/4: " + index);

        // Add a fraction at a specific index
        myArrayList.add(1, new Fraction(7, 8));

        // Print the ArrayList
        System.out.println(myArrayList);

        // Remove a fraction at a specific index
        Fraction removedFraction = myArrayList.remove(2);

        // Print the ArrayList
        System.out.println(myArrayList);

        // Print the removed fraction
        System.out.println("Removed fraction: " + removedFraction);
    }
}
